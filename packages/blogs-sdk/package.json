{
  "name": "@dishistech/blogs-sdk",
  "version": "1.0.3",
  "description": "Official SDK for the blogs.dishis.tech API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build:js": "tsup src/index.ts --format cjs,esm",
    "dev": "tsup src/index.ts --format cjs,esm --watch"
  },
  "keywords": ["api", "sdk", "blog", "dishistech"],
  "author": "blogs.dishis.tech",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dishis/DITBlogs-JS.git",
    "directory": "packages/blogs-sdk"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  }
}